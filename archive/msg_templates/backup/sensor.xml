<?xml version="1.0" ?>
<sensor>
    <BatteryState group="sensor">
        <capacity type="float" type_group="basic"/>
        <cell_voltage type="list" type_group="basic"/>
        <charge type="float" type_group="basic"/>
        <current type="float" type_group="basic"/>
        <design_capacity type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <location type="str" type_group="basic"/>
        <percentage type="float" type_group="basic"/>
        <power_supply_health type="int" type_group="basic"/>
        <power_supply_status type="int" type_group="basic"/>
        <power_supply_technology type="int" type_group="basic"/>
        <present type="bool" type_group="basic"/>
        <serial_number type="str" type_group="basic"/>
        <voltage type="float" type_group="basic"/>
    </BatteryState>
    <CameraInfo group="sensor">
        <binning_x type="int" type_group="basic"/>
        <binning_y type="int" type_group="basic"/>
        <distortion_model type="str" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <height type="int" type_group="basic"/>
        <roi type="RegionOfInterest" type_group="sensor"/>
        <width type="int" type_group="basic"/>
    </CameraInfo>
    <ChannelFloat32 group="sensor">
        <name type="str" type_group="basic"/>
        <values type="list" type_group="basic"/>
    </ChannelFloat32>
    <CompressedImage group="sensor">
        <data type="str" type_group="basic"/>
        <format type="str" type_group="basic"/>
        <header type="Header" type_group="std"/>
    </CompressedImage>
    <FluidPressure group="sensor">
        <fluid_pressure type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <variance type="float" type_group="basic"/>
    </FluidPressure>
    <Illuminance group="sensor">
        <header type="Header" type_group="std"/>
        <illuminance type="float" type_group="basic"/>
        <variance type="float" type_group="basic"/>
    </Illuminance>
    <Image group="sensor">
        <data type="str" type_group="basic"/>
        <encoding type="str" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <height type="int" type_group="basic"/>
        <is_bigendian type="int" type_group="basic"/>
        <step type="int" type_group="basic"/>
        <width type="int" type_group="basic"/>
    </Image>
    <Imu group="sensor">
        <angular_velocity type="Vector3" type_group="geometry"/>
        <angular_velocity_covariance type="list" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <linear_acceleration type="Vector3" type_group="geometry"/>
        <linear_acceleration_covariance type="list" type_group="basic"/>
        <orientation type="Quaternion" type_group="geometry"/>
        <orientation_covariance type="list" type_group="basic"/>
    </Imu>
    <JointState group="sensor">
        <effort type="list" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <name type="list" type_group="basic"/>
        <position type="list" type_group="basic"/>
        <velocity type="list" type_group="basic"/>
    </JointState>
    <Joy group="sensor">
        <axes type="list" type_group="basic"/>
        <buttons type="list" type_group="basic"/>
        <header type="Header" type_group="std"/>
    </Joy>
    <JoyFeedback group="sensor">
        <id type="int" type_group="basic"/>
        <intensity type="float" type_group="basic"/>
        <type type="int" type_group="basic"/>
    </JoyFeedback>
    <JoyFeedbackArray group="sensor">
        <array type="list" type_group="basic"/>
    </JoyFeedbackArray>
    <LaserEcho group="sensor">
        <echoes type="list" type_group="basic"/>
    </LaserEcho>
    <LaserScan group="sensor">
        <angle_increment type="float" type_group="basic"/>
        <angle_max type="float" type_group="basic"/>
        <angle_min type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <intensities type="list" type_group="basic"/>
        <range_max type="float" type_group="basic"/>
        <range_min type="float" type_group="basic"/>
        <ranges type="list" type_group="basic"/>
        <scan_time type="float" type_group="basic"/>
        <time_increment type="float" type_group="basic"/>
    </LaserScan>
    <MagneticField group="sensor">
        <header type="Header" type_group="std"/>
        <magnetic_field type="Vector3" type_group="geometry"/>
        <magnetic_field_covariance type="list" type_group="basic"/>
    </MagneticField>
    <MultiDOFJointState group="sensor">
        <header type="Header" type_group="std"/>
        <joint_names type="list" type_group="basic"/>
        <transforms type="list" type_group="basic"/>
        <twist type="list" type_group="basic"/>
        <wrench type="list" type_group="basic"/>
    </MultiDOFJointState>
    <MultiEchoLaserScan group="sensor">
        <angle_increment type="float" type_group="basic"/>
        <angle_max type="float" type_group="basic"/>
        <angle_min type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <intensities type="list" type_group="basic"/>
        <range_max type="float" type_group="basic"/>
        <range_min type="float" type_group="basic"/>
        <ranges type="list" type_group="basic"/>
        <scan_time type="float" type_group="basic"/>
        <time_increment type="float" type_group="basic"/>
    </MultiEchoLaserScan>
    <NavSatFix group="sensor">
        <altitude type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <latitude type="float" type_group="basic"/>
        <longitude type="float" type_group="basic"/>
        <position_covariance type="list" type_group="basic"/>
        <position_covariance_type type="int" type_group="basic"/>
        <status type="NavSatStatus" type_group="sensor"/>
    </NavSatFix>
    <NavSatStatus group="sensor">
        <service type="int" type_group="basic"/>
        <status type="int" type_group="basic"/>
    </NavSatStatus>
    <PointCloud group="sensor">
        <channels type="list" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <points type="list" type_group="basic"/>
    </PointCloud>
    <PointCloud2 group="sensor">
        <data type="str" type_group="basic"/>
        <fields type="list" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <height type="int" type_group="basic"/>
        <is_bigendian type="bool" type_group="basic"/>
        <is_dense type="bool" type_group="basic"/>
        <point_step type="int" type_group="basic"/>
        <row_step type="int" type_group="basic"/>
        <width type="int" type_group="basic"/>
    </PointCloud2>
    <PointField group="sensor">
        <count type="int" type_group="basic"/>
        <datatype type="int" type_group="basic"/>
        <name type="str" type_group="basic"/>
        <offset type="int" type_group="basic"/>
    </PointField>
    <Range group="sensor">
        <field_of_view type="float" type_group="basic"/>
        <header type="Header" type_group="std"/>
        <max_range type="float" type_group="basic"/>
        <min_range type="float" type_group="basic"/>
        <radiation_type type="int" type_group="basic"/>
        <range type="float" type_group="basic"/>
    </Range>
    <RegionOfInterest group="sensor">
        <do_rectify type="bool" type_group="basic"/>
        <height type="int" type_group="basic"/>
        <width type="int" type_group="basic"/>
        <x_offset type="int" type_group="basic"/>
        <y_offset type="int" type_group="basic"/>
    </RegionOfInterest>
    <RelativeHumidity group="sensor">
        <header type="Header" type_group="std"/>
        <relative_humidity type="float" type_group="basic"/>
        <variance type="float" type_group="basic"/>
    </RelativeHumidity>
    <Temperature group="sensor">
        <header type="Header" type_group="std"/>
        <temperature type="float" type_group="basic"/>
        <variance type="float" type_group="basic"/>
    </Temperature>
    <TimeReference group="sensor">
        <header type="Header" type_group="std"/>
        <source type="str" type_group="basic"/>
        <time_ref type="Time" type_group="std"/>
    </TimeReference>
</sensor>
